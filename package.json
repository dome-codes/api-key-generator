{
  "name": "Moba-api-generator",
  "version": "1.0.0",
  "description": "MobaRAG API Key Generator mit OpenAPI v1 Mock-API",
  "main": "mock-api.js",
  "type": "module",
  "scripts": {
    "start": "node mock-api.js",
    "dev": "node --watch mock-api.js",
    "frontend": "vite",
    "frontend:dev": "vite",
    "frontend:dev:no-debug": "SHOW_DEBUG=false vite",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview",
    "dev:all": "npm-run-all2 --parallel dev frontend:dev",
    "dev:all:no-debug": "SHOW_DEBUG=false npm-run-all2 --parallel dev frontend:dev",
    "dev:all:debug": "SHOW_DEBUG=true npm-run-all2 --parallel dev frontend:dev",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "keycloak-js": "^26.2.0",
    "pinia": "^3.0.3",
    "uuid": "^9.0.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "vite": "^7.0.3",
    "vue-tsc": "^2.0.6"
  },
  "keywords": [
    "api",
    "mock",
    "openapi",
    "Mobarag",
    "vue",
    "frontend"
  ],
  "author": "Domenic Schumacher",
  "license": "MIT"
}
